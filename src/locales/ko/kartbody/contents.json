    "kartDesc": {
        "group1":{
            "id": 1,
            "desc": "미래를 관통하는 카트바디.<br/>새로운 엔진과 최적화를 예고한다."
        },
        "group2":{
            "id": 2,
            "desc": "프로를 향한 첫걸음<br/>연습이 프로를 만든다."
        },
        "group3":{
            "id": 3,
            "desc": "그냥 거북이가 아니다.<br/>바다와 트랙 모두 정복할 해적이시다."
        },
        "group4":{
            "id": 4,
            "desc": "탁월한 밸런스!<br/>균형 잡힌 퍼포먼스가 펼쳐진다."
        },
        "group5":{
            "id": 5,
            "desc": "폭발적인 가속도!<br/>스타트부터 맹렬히 돌진한다."
        },
        "group6":{
            "id": 6,
            "desc": "뛰어난 최고 속도!<br/>직선 코스를 지배한다."
        },
        "group7":{
            "id": 7,
            "desc": "42.195.<br/>단 한순간도 지치지 않고, 백 번 천 번 내달린다."
        },
        "group8":{
            "id": 8,
            "desc": "날렵한 드리프트!<br/>세이버의 스키드 마크는 언제나 특별하다."
        },
        "group9":{
            "id": 9,
            "desc": "거침없는 폭풍 러시!<br/>최악의 악천후도 문제없다."
        },
        "group10":{
            "id": 10,
            "desc": "복엽기의 부활, 에이스의 귀환!<br/>이제 곧 미사일 폭격이 시작된다."
        },
        "group11":{
            "id": 11,
            "desc": "긴급 상황, 출동!<br/>트랙을 사수하고 반드시 생명을 지킨다.."
        },
        "group12":{
            "id": 12,
            "desc": "어두운 세상을 밝힐 신의 장막.<br/>환상적인 아름다움을 뽐내며 트랙을 내달린다."
        },
        "group13":{
            "id": 13,
            "desc": "트랙 위의 다크 히어로.<br/>냉혹하게 레이스를 지배한다."
        },
        "group14":{
            "id": 14,
            "desc": "레일을 벗어난 수레.<br/>바퀴를 읅매던 한계를 뛰어넘어 트랙을 질주한다."
        },
        "group15":{
            "id": 15,
            "desc": "독창적 아이디어와 다채로운 재미.<br/>트랙을 행복으로 물들인다."
        },
        "group16":{
            "id": 16,
            "desc": "신나게, 더 짜릿하게!<br/>언제 어디서나 누구든 즐겁게 달린다."
        },
        "group17":{
            "id": 17,
            "desc": "설명 준비중이에요."
        },
        "group18":{
            "id": 18,
            "desc": "설명 준비중이에요."
        },
        "group19":{
            "id": 19,
            "desc": "공기저항 이상 무!<br/>그 무엇도 레이싱을 방해하지 못한다."
        },
        "group20":{
            "id": 20,
            "desc": "설명 준비중이에요."
        },
        "group21":{
            "id": 21,
            "desc": "압도적 엔진, 압도적인 파워.<br/>레이스를 압도한다!"
        },
        "group22":{
            "id": 22,
            "desc": "달달한 당근의 힘!<br/>발바닥에 강력한 부스터가 샘솟는다."
        },
        "group23":{
            "id": 23,
            "desc": "변기로 위장한 비밀 병기.<br/>드디어... 드디어 출격이다!"
        },
        "group24":{
            "id": 24,
            "desc": "거대 도시를 섭렵한 베테랑<br/>어떤 목적지도 신속하게 돌파한다."
        },
        "group25":{
            "id": 25,
            "desc": "상상불가의 파괴력<br/>앞을 막는 자에게 자비란 없다."
        },
        "group26":{
            "id": 26,
            "desc": "포르쉐의 철학이 담긴 스포츠카<br/>편안하고 일상적인 놀라운 드라이빙을 제공한다."
        },
        "group27":{
            "id": 27,
            "desc": "미래지향적인 파워 튜닝<br/>독보적인 존재감으로 트랙을 뒤흔든다."
        },
        "group28":{
            "id": 28,
            "desc": "앉기만 하면 사건 해결!<br/>미스터리를 풀기 위해 어디든 간다."
        },
        "group29":{
            "id": 29,
            "desc": "사냥을 위해 태어난 하운드<br/>오늘은 승리를 사냥한다."
        },
        "group30":{
            "id": 30,
            "desc": "붐힐마을의 아침 당번<br/>신속 정확한 배달을 자랑한다."
        },
        "group31":{
            "id": 31,
            "desc": "BT21의 휴식 길라잡이!<br/>언제든 가장 편안한 휴식 공간으로 함께 가요."
        },
        "group32":{
            "id": 32,
            "desc": "BT21과의 즐거운 학교 생활!<br/>잊지 못할 학창시절의 즐거운 추억과 꿈을 싣고 함께 달려요."
        },
        "group33":{
            "id": 33,
            "desc": ""
        },
        "group34":{
            "id": 34,
            "desc": "트랙을 달리는 유령<br/>으스스한 공포감이 레이싱에 스며든다."
        },
        "group35":{
            "id": 35,
            "desc": "폭발적인 질주 머신<br/>그것이 곧 블랙 마크다."
        },
        "group36":{
            "id": 36,
            "desc": ""
        },
        "group37":{
            "id": 37,
            "desc": "투어 티켓 조기 매진!<br/>뉴욕을 보고 느끼고 즐긴다."
        },
        "group38":{
            "id": 38,
            "desc": "레이스 중에도 공부는 멈출 수 없다.<br/>π=3.141592653589793238462..."
        },
        "group39":{
            "id": 39,
            "desc": "거대 롤러의 습격!<br/>무엇이든 납작하게 깔아뭉갠다."
        },
        "group40":{
            "id": 40,
            "desc": "어두움 위 더 깊은 어둠<br/>아무도 모르게 다가가 암흑의 장막을 걷어낸다."
        },
        "group41":{
            "id": 41,
            "desc": "바다를 지배한 해적선<br/>카트로 탈바꿈하여 트랙을 습격한다!"
        },
        "group42":{
            "id": 42,
            "desc": "완벽한 튜닝의 성공<br/>유연한 움직임으로 레이싱을 완성한다."
        },
        "group43":{
            "id": 43,
            "desc": "안전 수칙 숙지 완료!<br/>트랙과 모두의 안전을 지켜낸다."
        },
        "group44":{
            "id": 44,
            "desc": "카트로 다시 태어난 상어<br/>포악함이 트랙을 뒤흔든다."
        },
        "group45":{
            "id": 45,
            "desc": "봄의 시작을 알리는 꽃마차<br/>트랙 위 꽃향기가 가득 퍼진다."
        },
        "group46":{
            "id": 46,
            "desc": "눈에 보이는 것, 그 이상. 승리는 이미 당신의 것이다."
        },
        "group47":{
            "id": 47,
            "desc": "무한한 가능성의 힘. 우주의 어둠을 헤치며 끝까지 나아간다."
        },
        "group48":{
            "id": 48,
            "desc": "스포츠카의 핵심을 유지한 로드스터<br/>일상의 주행을 최고의 즐거움으로 만든다."
        },
        "group49":{
            "id": 49,
            "desc": "팩토리의 지배자<br/>거친 눈보라와 빙판길은 무제조차 되지 않는다."
        },
        "group50":{
            "id": 50,
            "desc": "자율 주행의 혁신<br/>혼자서도 척척, 무슨 일이든 해낸다."
        },
        "group51":{
            "id": 51,
            "desc": "컴팩트 SUV의 스포츠카.<br/>플라이라인에서 포르쉐 DNA를 한눈에 알아볼 수 있다."
        },
        "group52":{
            "id": 52,
            "desc": "심장을 울리는 강력한 베이스.<br/>화려한 도심의 밤을 음악으로 물들인다."
        },
        "group53":{
            "id": 53,
            "desc": "중장비의 색다른 반란!<br/>공사장을 벗어나 새로운 매력을 뽐낸다."
        },
        "group54":{
            "id": 54,
            "desc": "데스, 데스!<br/>앞서가는 카트에 죽음의 그림자를 드리운다."
        },
        "group55":{
            "id": 55,
            "desc": "불법 정차, 고장난 카트는 아웃!<br/>레이스는 계속되어야 한다."
        },
        "group56":{
            "id": 56,
            "desc": "시공간을 초월한 카리스마<br/>범접할 수 없는 흑이 트랙에 퍼진다."
        },
        "group57":{
            "id": 57,
            "desc": "바다와 땅을 가리지 않는 수륙양용<br/>어디든 한계 없이 편안히 달린다."
        },
        "group58":{
            "id": 58,
            "desc": "아름다운 분홍빛의 플라밍고<br/>그 우아한 주행이 시작된다."
        },
        "group59":{
            "id": 59,
            "desc": "1960년대를 장악했던 GT카의 재탄생<br/>레전드가 돌아왔다."
        },
        "group60":{
            "id": 60,
            "desc": "이글이글 트랙을 달구는 솔라<br/>섬광을 번쩍이며 뜨겁게 질주한다."
        },
        "group61":{
            "id": 61,
            "desc": "증기 파워 가동!<br/>산업혁명에 이어 레이스의 혁명을 일으킨다."
        },
        "group62":{
            "id": 62,
            "desc": "자율주행의 대중화<br/>누구든 어디서든 쉽게 만나볼 수 있다."
        },
        "group63":{
            "id": 63,
            "desc": ""
        },
